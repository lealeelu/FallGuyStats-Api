<Window x:Class="FallGuysStats.Desktop.AllEpisodesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FallGuysStats.Desktop"
        mc:Ignorable="d"
        Title="All Episode Data" Height="414" Width="294" Background="#FF75D482">
    <Grid Margin="0,0,0,-6">
        <TextBox x:Name="boxEpisodeData" HorizontalAlignment="Left" Height="309" Margin="39,37,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="216" IsReadOnly="True"/>
        <TextBlock x:Name="tbEpisodeNumber" HorizontalAlignment="Left" Margin="86,5,0,0" Text="Episode" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="20" Width="132" TextAlignment="Center"/>
        <Button x:Name="btnPrevious" Content="Previous" HorizontalAlignment="Left" Margin="51,361,0,0" VerticalAlignment="Top" Width="71" IsEnabled="False" Click="btnPrevious_Click"/>
        <Button Content="Next" HorizontalAlignment="Left" Margin="166,361,0,0" VerticalAlignment="Top" Width="69" Click="Button_Click"/>

    </Grid>
</Window>
