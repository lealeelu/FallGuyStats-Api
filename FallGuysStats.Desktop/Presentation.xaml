        <TextBox x:Name="boxPrimary" HorizontalAlignment="Left" Margin="366,86,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="452" Grid.ColumnSpan="2" Height="570"/>

